    "version": "0.2.0",
    "configurations": [
        {
            "name": "Discord Bot",
            "type": "python",
            "request": "launch",
            // "args": ["--",""],
            "program": "${workspaceFolder}/discordbot.py",
            "console": "integratedTerminal",
        },
        {
            "name": "Scrape OpenSea (Node)",
            "program": "${workspaceFolder}/opensea_scrape/opensea_offers.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "cwd": "${workspaceFolder}/opensea_scrape/"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}